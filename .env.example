# OIDC Broker Configuration
# Copy this file to .env and configure for your environment

# Auth Code Store (use Redis in production)
APP_AUTH_CODE_REDIS_STORE_ENABLED=0
APP_AUTH_CODE_REDIS_STORE_PREFIX=authcode:

# OIDC Configuration
APP_OIDC_ISSUER=http://localhost:3000
APP_OIDC_CLIENTS=[]

# Redis Configuration (optional, enable for production)
APP_REDIS_ENABLED=0
APP_REDIS_HOST=localhost
APP_REDIS_PORT=6379
APP_REDIS_PROTO=redis
APP_REDIS_PASS=
APP_REDIS_DB=0

# Session Configuration
APP_SESSION_SECRET=secret
APP_SESSION_REDIS_STORE_ENABLED=0
APP_SESSION_REDIS_STORE_PREFIX=session:

# =============================================================================
# OAuth Providers Configuration (NEW - Multi-Provider Support)
# =============================================================================
# Configure multiple OAuth 2.0 providers as a JSON array.
# Supported providers: twitter, github, google
#
# Each provider requires:
#   - name: Provider type (twitter, github, google)
#   - client_id: OAuth client ID
#   - client_secret: OAuth client secret
#   - callback_url: OAuth callback URL
#
# Optional fields (for custom/enterprise providers):
#   - auth_url: Custom authorization endpoint
#   - token_url: Custom token endpoint
#   - user_url: Custom user info endpoint
#   - scopes: Custom OAuth scopes
#
# Example with multiple providers:
# APP_PROVIDERS='[{"name":"twitter","client_id":"xxx","client_secret":"xxx","callback_url":"http://localhost:3000/auth/twitter/callback"},{"name":"github","client_id":"xxx","client_secret":"xxx","callback_url":"http://localhost:3000/auth/github/callback"},{"name":"google","client_id":"xxx","client_secret":"xxx","callback_url":"http://localhost:3000/auth/google/callback"}]'
#
# Routes are automatically created:
#   - /auth/twitter and /auth/twitter/callback
#   - /auth/github and /auth/github/callback
#   - /auth/google and /auth/google/callback

APP_PROVIDERS=[]

# =============================================================================
# Legacy Twitter Configuration (DEPRECATED - use APP_PROVIDERS instead)
# =============================================================================
# These settings are still supported for backward compatibility.
# If APP_PROVIDERS is empty but these are set, a Twitter provider will be created.
APP_TWITTER_CALLBACK_URL=http://localhost:3000/auth/twitter/callback
APP_TWITTER_CLIENT_ID=
APP_TWITTER_CLIENT_SECRET=

# JWT Signing Key
# Generate with: openssl genrsa 2048 | base64 -w0
APP_KEY_ID=aabbcccdd-2222-4444-6666-000000000000
APP_KEY_PRIVATE_

# Debug Configuration
APP_DEBUG_ENABLED=1
APP_DEBUG_BASE_URL=http://localhost:3000
APP_DEBUG_INSPECTION_ENABLED=0

# Cognito Integration (optional, for testing)
APP_DEBUG_COGNITO_ENABLED=0
APP_DEBUG_COGNITO_DOMAIN=
APP_DEBUG_COGNITO_CLIENT_ID=
APP_DEBUG_COGNITO_CLIENT_SECRET=
